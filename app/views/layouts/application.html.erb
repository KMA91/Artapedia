<!DOCTYPE html>
<html>
<head>
  <title>Art</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<% if flash[:errors] %>
    <% flash[:errors].each do |e|%>
      <div class= "alert alert-danger" style="color:red;">
        <strong><%= e %></strong>
      </div>
    <% end %>
  <% end %>
<%= yield %>

</body>
</html>
